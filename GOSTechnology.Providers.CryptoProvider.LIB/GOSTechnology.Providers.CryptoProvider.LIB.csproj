    <Version>1.0.0-preview1</Version>
    <Authors>GabrielMotakiru</Authors>
    <Company>GOSTechnology</Company>
    <Description>Crypto management provider in AES 128bits.</Description>
    <PackageIcon>icon.png</PackageIcon>
    <PackageIconUrl />
    <PackageProjectUrl>https://github.com/gabrielmotakiru/GOSTechnology.Providers.CryptoProvider.LIB</PackageProjectUrl>
    <RepositoryUrl>https://github.com/gabrielmotakiru/GOSTechnology.Providers.CryptoProvider.LIB</RepositoryUrl>
    <RepositoryType>MIT License</RepositoryType>
    <PackageTags>CryptoProvider</PackageTags>
